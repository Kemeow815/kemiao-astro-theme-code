        "@directus/sdk": "^19.1.0",
        "@expressive-code/plugin-text-markers": "^0.41.2",
        "@iconify-json/carbon": "^1.2.8",
        "@iconify-json/mdi": "^1.2.3",
        "@tailwindcss/vite": "^4.1.7",
        "astro": "5.8.0",
        "astro-expressive-code": "^0.41.2",
        "astro-icon": "^1.1.5",
        "astro-robots-txt": "^1.0.0",
        "date-fns": "^4.1.0",
        "mdast-util-to-string": "^4.0.0",
        "pocketbase": "^0.26.0",
